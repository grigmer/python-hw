f = open("C:\Users\Grigory\OneDrive\Рабочий стол\pythondz\файлСФ1.txt", 'r')
z = f.readline()
z = z[0:-1]
n = int(z)
m = 0
for i in range(n-1):
    y = f.readline()
    y = y[0:-1]
    x = int(y)
    if i == 0:
        m = x
    else:
        if x > m:
            m = x
z = f.readline()
z = z[0:-1]
n = int(z)
if n > m:
    m = n 
h = 0
while m%10 == 0:
    h += 1
    m//10
print(h)